<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.madehuge_nishant.videostreamer">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/lauch"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@drawable/lauch"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Category_detail"></activity>
        <activity android:name="com.madehuge_nishant.videostreamer.Reset_password" />
        <activity
            android:name="com.madehuge_nishant.videostreamer.Article_detail"
            android:parentActivityName="com.madehuge_nishant.videostreamer.Article_detail" />
        <activity
            android:name="com.madehuge_nishant.videostreamer.Edit_profile"
            android:parentActivityName="com.madehuge_nishant.videostreamer.MainActivity" />
        <activity
            android:name="com.madehuge_nishant.videostreamer.Upload_article"
            android:parentActivityName="com.madehuge_nishant.videostreamer.MainActivity" />
        <activity
            android:name="com.madehuge_nishant.videostreamer.Upload_video"
            android:parentActivityName="com.madehuge_nishant.videostreamer.MainActivity" />
        <activity android:name="com.madehuge_nishant.videostreamer.Register" />
        <activity android:name="com.madehuge_nishant.videostreamer.Login" />
        <activity android:name="com.madehuge_nishant.videostreamer.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.madehuge_nishant.videostreamer.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.madehuge_nishant.videostreamer.Video_detail"
            android:parentActivityName="com.madehuge_nishant.videostreamer.MainActivity" />
        <activity
            android:name="com.madehuge_nishant.videostreamer.About_us"
            android:parentActivityName="com.madehuge_nishant.videostreamer.MainActivity" />

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity
            android:name="com.madehuge_nishant.videostreamer.VideosByCat"
            android:parentActivityName="com.madehuge_nishant.videostreamer.MainActivity"></activity>
        <activity
            android:name="com.madehuge_nishant.videostreamer.History"
            android:parentActivityName="com.madehuge_nishant.videostreamer.MainActivity" />
        <activity
            android:name="com.madehuge_nishant.videostreamer.Edit_video"
            android:parentActivityName="com.madehuge_nishant.videostreamer.History" />
        <activity
            android:name="com.madehuge_nishant.videostreamer.update_pictures"
            android:parentActivityName="com.madehuge_nishant.videostreamer.History" />
        <activity
            android:name="com.madehuge_nishant.videostreamer.Picture_by_cat"
            android:parentActivityName="com.madehuge_nishant.videostreamer.MainActivity" />
    </application>

</manifest>